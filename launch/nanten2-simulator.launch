  <node pkg = "necst_ros3"
  	name = "dome_door_left_position"
	type = "dome_door_left_position.py" />

  <node pkg = "necst_ros3"
  	name = "dome_door_right_drive"
	type = "dome_door_right_drive.py" />

  <node pkg = "necst_ros3"
  	name = "dome_door_right_position"
