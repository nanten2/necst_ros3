<launch>
  
  <node pkg="pci_boards_driver" 
        name="cpz2724m_rsw0" 
        type="cpz2724m.py"
        output="screen"
        machine="rxfac1">
    <param name="~rsw_id" value="0" />
  </node>

  <node pkg="pci_boards_driver" 
        name="cpz2724m_rsw1" 
        type="cpz2724m.py"
        output="screen"
        machine="rxfac1">
    <param name="~rsw_id" value="1" />
  </node>
  
  <node pkg="pci_boards_driver" 
        name="cpz2724m_rsw2" 
        type="cpz2724m.py"
        output="screen"
        machine="rxfac1">
    <param name="~rsw_id" value="2" />
  </node>

  <node pkg="pci_boards_driver"
        name="cpz6204_rsw0"
        type="cpz6204.py"
        output="screen"
        machine="rxfac1">
    <param name="~rsw_id" value="0" />
  </node>

