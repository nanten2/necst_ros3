<launch>

  <node pkg = "necst_ros3"
      name = "hot_position"
      type = "hot_position.py">
  </node>

  <node pkg = "necst_ros3"
      name = "hot_position_sim"
      type = "hot_position_sim.py">
  </node>

  <test pkg = "necst_ros3"
  test-name = "test_hot_position"
  type = "test_hot_position.py">
    <param name="timeout" textfile="$(find necst_ros3)/test/timeout.param" />
  </test>

</launch>

