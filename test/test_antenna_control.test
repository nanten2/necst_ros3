<launch>

  <node pkg = "necst_ros3"
      name = "antenna_control"
      type = "antenna_control.py">
    <param name="name_topic_from" value="antenna_control_input_sim" />
  </node>

  <test pkg = "necst_ros3"
  test-name = "test_antenna_control"
  type = "test_antenna_control.py">
    <param name="timeout" textfile="$(find necst_ros3)/test/timeout.param" />
  </test>

</launch>
